<?xml version="1.0" encoding="UTF-8"?>
<protectionInstantiationsList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://protectionInstantiation.kb.esp.security.polito.it" xsi:schemaLocation="../schema/protectionInstantiation.xsd">
	
	<!-- 80 instr. to init opaque list -->
	<!-- per string in literal in function: 5 instr. to call the string encoder -->
	<!-- per each char in the string literals in the function: 8 instr. to decode the char in the string encoder -->
	<!-- per each integer literal in the function: 10 instr to encode the value -->
 	<protectionInstantiation>
		<protectionInstantiationName>Tigress Source Code Literal Obfuscation</protectionInstantiationName>
		<protectionName>sourceCodeLiteralObfuscation</protectionName>
		<toolCommand>--Transform=InitOpaque --Functions=#func# --Prefix=_ESP_#func#_ --InitOpaqueStructs=list --Transform=EncodeLiterals --Prefix=_ESP_#func#_ --Functions=#func#</toolCommand>
		<clientTimeOverhead>(80 + 8 * sum_total_dimension_strings_lit_in_func + 5 * no_strings_lit_in_func + 10 * no_int_lit_in_func)  / 1000<!--  nr_ins_static--></clientTimeOverhead> 
		<serverTimeOverhead>0</serverTimeOverhead>
 		<clientMemoryOverhead>0</clientMemoryOverhead>
 		<serverMemoryOverhead>0</serverMemoryOverhead>
 		<networkOverhead>0</networkOverhead>
	</protectionInstantiation>


</protectionInstantiationsList>
