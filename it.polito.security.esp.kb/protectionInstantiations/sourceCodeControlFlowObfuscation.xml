<?xml version="1.0" encoding="UTF-8"?>
<protectionInstantiationsList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://protectionInstantiation.kb.esp.security.polito.it" xsi:schemaLocation="../schema/protectionInstantiation.xsd">
	
	
 	<protectionInstantiation>
		<protectionInstantiationName>Tigress Source Code Control Flow Flattening</protectionInstantiationName>
		<protectionName>sourceCodeControlFlowObfuscation</protectionName>
		<toolCommand>--Transform=Flatten --Functions=#func# --Prefix=_ESP_#func#_ --FlattenRandomizeBlocks=true</toolCommand>
		<clientTimeOverhead>3 / 8</clientTimeOverhead> <!--  (10 + (3 * nr_inst_st / 8)) / nr_inst_st = 10 fixed + 3 per basic block-->
		<serverTimeOverhead>0</serverTimeOverhead>
 		<clientMemoryOverhead>0</clientMemoryOverhead>
 		<serverMemoryOverhead>0</serverMemoryOverhead>
 		<networkOverhead>0</networkOverhead>
	</protectionInstantiation>

	<protectionInstantiation>
		<protectionInstantiationName>Tigress Source Code Opaque Predicates</protectionInstantiationName>
		<protectionName>sourceCodeControlFlowObfuscation</protectionName>
		<toolCommand>--Transform=InitOpaque --Functions=#func# --Prefix=_ESP_#func#_ --InitOpaqueStructs=list --Transform=AddOpaque --Functions=#func# --Prefix=_ESP_#func#_ --AddOpaqueCount=1 --AddOpaqueKinds=bug</toolCommand>
		<clientTimeOverhead>84  / 1000<!--nr_ins_static!--></clientTimeOverhead> <!-- 80 fixed init + 4 per opaque, with 1 one opaque per func and init in main = 4 / nr_insr_st -->
		<serverTimeOverhead>0</serverTimeOverhead>
 		<clientMemoryOverhead>0</clientMemoryOverhead>
 		<serverMemoryOverhead>0</serverMemoryOverhead>
 		<networkOverhead>0</networkOverhead>
	</protectionInstantiation>

</protectionInstantiationsList>
